.month-zankai{
	position: absolute;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
	/* border:solid 1rpx #000000; */
	height: 60rpx;
	width: 500rpx;
	-webkit-animation: zankai 4s;
	        animation: zankai 4s;
}
@-webkit-keyframes zankai{
from{width:500rpx;}
to{
		widht:0rpx;
}
}
@keyframes zankai{
from{width:500rpx;}
to{
		widht:0rpx;
}
}
